<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name), :html => {:class => "form-horizontal"}, :html => {:class => "well"} ) do |f| %>

<%= devise_error_messages! %>

<fieldset>
	<legend>Sign up</legend>
	<%= f.input :first_name, autofocus: true %>
	<%= f.input :last_name %>
	<%= f.input :profile_name %>
	<%= f.input :password %>
	<%= f.input :password_confirmation %>
	<%= f.button :submit, "Sign up" %>
</fieldset>

<% end %>

<%= render "devise/shared/links" %>
